\pgfarrowsdeclare{triangle}{triangle}
{
    \pgfarrowsleftextend{0pt}
    \pgfarrowsrightextend{+4.5\pgfutil@tempdima}%
}
{
    \pgfutil@tempdima=0.8pt% % Adjust the size here for smaller arrowhead
    \pgfsetdash{}{+0pt}%
    \pgfsetmiterjoin%
    \pgfpathmoveto{\pgfqpoint{0pt}{-3.5\pgfutil@tempdima}}%
    \pgfpathlineto{\pgfqpoint{5.5\pgfutil@tempdima}{0pt}}% % Reversed direction
    \pgfpathlineto{\pgfqpoint{0pt}{3.5\pgfutil@tempdima}}%
    \pgfpathclose%
    \pgfusepathqstroke%
}

\pgfarrowsdeclare{ltriangle}{ltriangle}
{
    \pgfarrowsleftextend{-2\pgfutil@tempdima}%
    \pgfarrowsrightextend{+6\pgfutil@tempdima}%
}
{
    \pgfutil@tempdima=0.8pt % Adjust the size here for smaller arrowhead
    \pgfsetdash{}{+0pt}%
    \pgfsetmiterjoin%
    % Draw vertical line part
    \pgfpathmoveto{\pgfqpoint{-2.5\pgfutil@tempdima}{-3.5\pgfutil@tempdima}}%
    \pgfpathlineto{\pgfqpoint{-2.5\pgfutil@tempdima}{3.5\pgfutil@tempdima}}%
    \pgfusepathqstroke%
    % Draw triangle part
    \pgfpathmoveto{\pgfqpoint{0pt}{-3.5\pgfutil@tempdima}}%
    \pgfpathlineto{\pgfqpoint{5.5\pgfutil@tempdima}{0pt}}%
    \pgfpathlineto{\pgfqpoint{0pt}{3.5\pgfutil@tempdima}}%
    \pgfpathclose%
    \pgfusepathqstroke%
}

\pgfarrowsdeclare{ptriangle}{ptriangle}
{
    \pgfarrowsleftextend{-2\pgfutil@tempdima}%
    \pgfarrowsrightextend{+6\pgfutil@tempdima}%
}
{
    \pgfutil@tempdima=0.8pt % Adjust the size here for the dot size
    \pgfsetdash{}{+0pt}%
    \pgfsetmiterjoin%
    % Draw dot part
    \pgfpathcircle{\pgfqpoint{-3\pgfutil@tempdima}{0pt}}{1.3\pgfutil@tempdima}%
    \pgfusepathqfillstroke%
    % Draw triangle part
    \pgfpathmoveto{\pgfqpoint{0pt}{-3.5\pgfutil@tempdima}}%
    \pgfpathlineto{\pgfqpoint{5.5\pgfutil@tempdima}{0pt}}%
    \pgfpathlineto{\pgfqpoint{0pt}{3.5\pgfutil@tempdima}}%
    \pgfpathclose%
    \pgfusepathqstroke%
}


